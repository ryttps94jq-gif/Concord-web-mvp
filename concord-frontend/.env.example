# Concord Frontend - Environment Configuration
# Copy this file to .env.local and update values

# Backend API URL
# For production: https://yourdomain.com (same as your domain)
# For local dev: http://localhost:5050
NEXT_PUBLIC_API_URL=http://localhost:5050

# WebSocket URL (defaults to API URL if not set)
# For production: https://yourdomain.com
# For local dev: http://localhost:5050
NEXT_PUBLIC_SOCKET_URL=http://localhost:5050

# App version (shown in UI)
NEXT_PUBLIC_APP_VERSION=2.0.0

# Environment
NODE_ENV=development
